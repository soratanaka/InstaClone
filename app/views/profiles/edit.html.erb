<%= form_with model: @user, url: profile_path, local: true do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, class: 'form-control mb-3' %>

  <%= f.label :avatar %>
  <%= f.file_field :avatar,class: 'form-control mb-3' %>
  <%= f.hidden_field :avatar_cache %>

  <%= f.submit "更新", class: "btn btn-primary"%>
<%end%>